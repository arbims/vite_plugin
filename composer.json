{
    "name": "arbims/vite-plugin",
    "description": "VitePlugin plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "php": ">=7.4",
        "cakephp/cakephp": "^5.0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5 || ^9.3"
    },
    "autoload": {
        "psr-4": {
            "VitePlugin\\": "src/"
        }
    },
    "authors": [
        {
          "name": "arbi msahel"
        }
      ],
    "minimum-stability": "dev",
    "prefer-stable" : true,
    "autoload-dev": {
        "psr-4": {
            "VitePlugin\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
